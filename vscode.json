{
    "editor.fontSize": 16,
    "zenMode.hideTabs": false,
    "window.titleBarStyle": "custom",
    "editor.tabSize": 2,
    "editor.fontLigatures": true,
    "editor.minimap.enabled": false,
    "editor.fontFamily": "Pragmata Pro",
    "window.zoomLevel": 0,
    "editor.wordWrap": "on",
    "editor.cursorBlinking": "phase",
    "editor.renderLineHighlight": "gutter",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.configuration.checkProjectSettingsExclusions": false,
    "debug.console.fontFamily": "Pragmata Pro",
    "java.errors.incompleteClasspath.severity": "ignore",
    "C_Cpp.updateChannel": "Insiders",
    "explorer.confirmDelete": false,
    "workbench.startupEditor": "newUntitledFile",
    "git.autofetch": true,
    "files.insertFinalNewline": true,
    "workbench.iconTheme": "material-icon-theme",
    "[java]": {},
    "PandocCiter.DefaultBib": "paper.bib",
    "pandoc.pdfOptString": "--filter pandoc-citeproc --biblio paper.bib -t latex --highlight-style pygments --standalone --variable documentclass=assets/latex/ieee --csl=assets/csl/ieee.csl",
    "cSpell.userWords": [
        "Arduino",
        "Higibontham",
        "archifamous",
        "structs",
        "videogame",
        "videogames"
    ],
    "explorer.confirmDragAndDrop": false,
    "breadcrumbs.enabled": true,
    "workbench.colorTheme": "Dracula"
}
